RewriteEngine On

# If the file exists, serve it directly
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^ - [L]

# If the directory exists, serve it directly
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# Handle _next directory requests
RewriteRule ^musikquiz/_next/(.*) _next/$1 [L]

# Handle static files
RewriteRule ^musikquiz/static/(.*) static/$1 [L]

# All other requests go to index.php
RewriteRule ^ index.php [L] 